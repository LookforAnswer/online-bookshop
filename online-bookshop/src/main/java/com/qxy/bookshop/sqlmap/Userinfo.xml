"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qxy.bookshop.dao.LoginDao">
         
         <insert id="insertUserInfo"  parameterType="userInfo" >
             insert into userinfo(  
         id,
         username,
         password) 
         values 
         (#{id},      
         #{username},
         #{password})
         </insert>
         
         <select id="queryByEntity">
         	
         </select>
         
</mapper>